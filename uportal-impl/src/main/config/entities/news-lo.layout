
<layout script="classpath://org/jasig/portal/io/import-layout_v2-6.crn" username="news-lo">
  <root unremovable="Y" immutable="N" hidden="N" name="Root folder">
    <header unremovable="Y" immutable="Y" hidden="N" name="Header folder">
      <channel fname="header"/>
      <channel fname="portal/login/general"/>
      <channel fname="session-locales-selector"/>
    </header>
    <footer unremovable="N" immutable="N" hidden="N" name="Footer folder">
      <channel fname="footer"/>
    </footer>
    <tab unremovable="N" immutable="N" hidden="N" name="Useful News">
      <param>
        <name>cp:moveAllowed</name>
        <value>false</value>
      </param>
      <column unremovable="N" immutable="N" hidden="N" name="Column">
        <structure-attribute type="folder">
          <name>width</name>
          <value>30%</value>
        </structure-attribute>
        <structure-attribute type="folder">
          <name>width</name>
          <value>30%</value>
        </structure-attribute>
        <channel fname="salon.com"/>
        <channel fname="word-of-the-day"/>
      </column>
      <column unremovable="N" immutable="N" hidden="N" name="Column">
        <structure-attribute type="folder">
          <name>width</name>
          <value>70%</value>
        </structure-attribute>
        <structure-attribute type="folder">
          <name>width</name>
          <value>70%</value>
        </structure-attribute>
        <channel fname="motley-fool"/>
        <channel fname="unicon-news"/>
      </column>
    </tab>
  </root>
</layout>