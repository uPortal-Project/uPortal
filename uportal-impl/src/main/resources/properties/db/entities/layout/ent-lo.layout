
<layout script="classpath://org/jasig/portal/io/import-layout_v2-6.crn" username="ent-lo">
  <root unremovable="Y" immutable="N" hidden="N" name="Root folder">
    <header unremovable="Y" immutable="Y" hidden="N" name="Header folder">
      <channel fname="header"/>
      <channel fname="portal/login/general"/>
      <channel fname="session-locales-selector"/>
    </header>
    <footer unremovable="N" immutable="N" hidden="N" name="Footer folder">
      <channel fname="footer"/>
    </footer>
    <tab unremovable="N" immutable="N" hidden="N" name="Entertainment">
      <param>
        <name>cp:editAllowed</name>
        <value>false</value>
      </param>
      <column unremovable="N" immutable="N" hidden="N" name="Column">
        <structure-attribute type="folder">
          <name>width</name>
          <value>40%</value>
        </structure-attribute>
        <channel fname="number-guessing-game">
          <param>
            <name>cp:deleteAllowed</name>
            <value>false</value>
          </param>
          <param>
            <name>cp:moveAllowed</name>
            <value>false</value>
          </param>
          <param>
            <name>maxNum</name>
            <value>100</value>
          </param>
          <param>
            <name>minNum</name>
            <value>1</value>
          </param>
        </channel>
        <channel fname="minesweeper"/>
      </column>
      <column unremovable="N" immutable="N" hidden="N" name="Column">
        <structure-attribute type="folder">
          <name>width</name>
          <value>60%</value>
        </structure-attribute>
        <channel fname="daily-business-cartoon"/>
      </column>
    </tab>
  </root>
</layout>