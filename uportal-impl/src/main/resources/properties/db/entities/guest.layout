
<layout script="classpath://org/jasig/portal/io/import-layout_v2-6.crn" username="guest">
  <root unremovable="Y" immutable="N" hidden="N" name="Root folder">
    <header unremovable="Y" immutable="Y" hidden="N" name="Header folder">
      <channel fname="header"/>
      <channel fname="portal/login/general"/>
      <channel fname="session-locales-selector"/>
    </header>
    <tab unremovable="N" immutable="N" hidden="N" name="Main">
      <column unremovable="N" immutable="N" hidden="N" name="Column 1">
        <channel fname="uportal-powered-sites"/>
      </column>
      <column unremovable="N" immutable="N" hidden="N" name="Column 2">
        <channel fname="uportal-home-page"/>
      </column>
    </tab>
    <tab unremovable="N" immutable="N" hidden="N" name="Misc">
      <column unremovable="N" immutable="N" hidden="N" name="Column 1">
        <channel fname="minesweeper"/>
      </column>
      <column unremovable="N" immutable="N" hidden="N" name="Column 2">
        <channel fname="word-of-the-day"/>
      </column>
    </tab>
    <tab unremovable="N" immutable="N" hidden="N" name="News">
      <column unremovable="N" immutable="N" hidden="N" name="Column 1">
        <channel fname="salon.com"/>
      </column>
      <column unremovable="N" immutable="N" hidden="N" name="Column 2">
        <channel fname="unicon-news"/>
        <channel fname="motley-fool"/>
      </column>
    </tab>
    <footer unremovable="Y" immutable="Y" hidden="N" name="Footer folder">
      <channel fname="footer"/>
    </footer>
  </root>
</layout>