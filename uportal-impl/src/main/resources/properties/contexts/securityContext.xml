<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="xalanAuthorizationHelper" class="org.jasig.portal.security.xslt.XalanAuthorizationHelperBean">
        <property name="channelRegistryStore" ref="channelRegistryStore" />
    </bean>
    
    <bean id="maxInactiveFilter" class="org.jasig.portal.security.MaxInactiveFilter">
        <property name="personManager" ref="personManager" />
    </bean>
</beans>