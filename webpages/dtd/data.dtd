<!ELEMENT data (table+) >
<!ELEMENT table (name, rows) >
<!ATTLIST table
   sinceMajor  CDATA #REQUIRED
   sinceMinor  CDATA #REQUIRED>

<!ELEMENT rows (row+)>
<!ELEMENT row (column+)>
<!ATTLIST row
   sinceMajor  CDATA #IMPLIED
   sinceMinor  CDATA #IMPLIED>
<!ELEMENT column (name, value)>
<!ELEMENT name (#PCDATA)>
<!ELEMENT value (#PCDATA|sequence)*>

<!ELEMENT sequence (#PCDATA)>
<!ATTLIST sequence
	id CDATA #REQUIRED>
