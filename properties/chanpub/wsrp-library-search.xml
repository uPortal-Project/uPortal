<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE channel-definition SYSTEM "channelDefinition.dtd">
<!-- This is a remote portlet via WSRP
-->
	<channel-definition>
	    <title>Library Search</title>
	    <name>Library Search</name>
	    <fname>wsrp-library-search</fname>
	    <desc>Library Search portlet</desc>
	    <type>Portlet</type>
	    <class>org.jasig.portal.channels.portlet.CPortletAdapter</class>
	    <timeout>60000</timeout>
	    <hasedit>N</hasedit>
	    <hashelp>N</hashelp>
	    <hasabout>N</hasabout>
	    <secure>N</secure>
	    <locale>en_US</locale>

	    <categories>
	        <category>Applications</category>
	    </categories>

	    <groups>
	        <group>Everyone</group>
	    </groups>

	    <parameters>

	        <!--
	            The syntax of the ID is [portlet-context-name].[portlet-name] 
	        -->

	        <parameter>
	            <name>portletDefinitionId</name>
	            <value>proxyportlet.ProxyPortlet</value>
	            <description>
	                Identifies the portlet deployed within the portlet container
	            </description>
	            <ovrd>N</ovrd>
	        </parameter>
	        
	        <!--
	            Portlet entity preference names will be prepended with "PORTLET." 
	        -->

	        <parameter>
	            <name>PORTLET.wsrp_markup_url</name>
	            <value>http://www.jafer.org/wsrp/wsrp4j/WSRPBaseService</value>
	            <description>Markup Interface URL</description>
	            <ovrd>N</ovrd>
	        </parameter>

	        <parameter>
	            <name>PORTLET.wsrp_service_desc_url</name>
	            <value>
	                http://www.jafer.org/wsrp/wsrp4j/WSRPServiceDescriptionService
	            </value>
	            <description>Service Description Interface URL</description>
	            <ovrd>N</ovrd>
	        </parameter>

	        <parameter>
	            <name>PORTLET.wsrp_reg_url</name>

	            <value>
	                http://www.jafer.org/wsrp/wsrp4j/WSRPRegistrationService
	            </value>
	            <description>Registration Interface URL</description>
	            <ovrd>N</ovrd>
	        </parameter>

	        <parameter>
	            <name>PORTLET.wsrp_portlet_mgmt_url</name>

	            <value>
	                http://www.jafer.org/wsrp/wsrp4j/WSRPPortletManagementService
	            </value>
	            <description>Portlet Management Interface URL</description>
	            <ovrd>N</ovrd>
	        </parameter>

	        <parameter>
	            <name>PORTLET.wsrp_portlet_handle</name>
	            <value>JaferPortlet.SearchPortlet</value>
	            <description>Portlet Handle on the remote portal</description>
	            <ovrd>Y</ovrd>
	        </parameter>
	    </parameters>
	</channel-definition>