<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE channel-definition PUBLIC "-//uPortal//Channel Definition/EN" "channelDefinition.dtd">

<!-- This is a UW/Eric Dalquist bookmarks portlet.
 -->

<channel-definition>
    <channelId>116</channelId>
    <title>Bookmarks</title>
    <name>Bookmarks</name>
    <fname>pbookmarks</fname>
    <desc>UW Eric Dalquist bookmarks portlet</desc>
    <type>Portlet</type>
    <class>org.jasig.portal.channels.portlet.CPortletAdapter</class>
    <timeout>60000</timeout>

    <hasedit>N</hasedit>
    <hashelp>N</hashelp>
    <hasabout>N</hasabout>

    <secure>N</secure>
    <locale>en_US</locale>

    <categories>
        <category>Development</category>
    </categories>

    <groups>
        <group>Everyone</group>
    </groups>

    <parameters>

        <!-- The syntax of the ID is [portlet-context-name].[portlet-name] -->
        <parameter>
            <name>portletDefinitionId</name>
            <value>BookmarksPortlet.BookmarksPortlet</value>
            <description>Identifies the portlet deployed within the portlet container</description>
            <ovrd>N</ovrd>
        </parameter>

    </parameters>

</channel-definition>
