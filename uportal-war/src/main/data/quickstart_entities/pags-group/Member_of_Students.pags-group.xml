<!--
  Group member_of_students: "Member of Students (PAGS)"
  All IPersons that are in the Students Group are members of this group
-->
<pags-group script="classpath://org/jasig/portal/io/import-pags-group_v4-1.crn">
  <name>Member of Students (PAGS)</name>
  <description>Member of Students group</description>
  <selection-test>
    <test-group>
      <test>
          <tester-class>org.jasig.portal.groups.pags.testers.AdHocGroupTester</tester-class>
          <includes>Students</includes>
          <includes>English Majors</includes>
          <excludes>BannedStudents</excludes>
          <excludes>SummerSessionStudents</excludes>
      </test>
    </test-group>
  </selection-test>
</pags-group>

