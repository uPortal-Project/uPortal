<layout xmlns:dlm="http://www.uportal.org/layout/dlm"
  script="classpath://org/jasig/portal/io/import-layout_v3-2.crn"
  username="pilot-lo">
  <folder ID="s1" hidden="false" immutable="false" name="Root folder"
    type="root" unremovable="true">

    <folder ID="s11" hidden="true" immutable="true" name="Header folder"
      type="header" unremovable="true">
      <channel fname="fragment-admin-exit" unremovable="false"
        hidden="false" immutable="false" ID="n3" />
    </folder>


    <folder ID="s12" hidden="false" dlm:deleteAllowed="false"
      dlm:editAllowed="false" dlm:moveAllowed="false" immutable="true"
      name="Award Record" type="regular" unremovable="true">
      <structure-attribute type="folder">
        <name>tabGroup</name>
        <value>Pilot</value>
      </structure-attribute>
      <folder ID="s121" dlm:deleteAllowed="false"
        dlm:editAllowed="false" dlm:moveAllowed="false" hidden="false"
        immutable="true" name="registration folder" type="regular"
        unremovable="true">
        <channel fname="awardrecord" dlm:deleteAllowed="false"
          dlm:editAllowed="false" dlm:moveAllowed="false" unremovable="true"
          hidden="false" immutable="true" ID="n1211" />
      </folder>
      <!-- A comment. With this present, folder s122 won't be imported -->
      <folder ID="s122" dlm:deleteAllowed="false"
        dlm:editAllowed="false" dlm:moveAllowed="false" hidden="false"
        immutable="true" name="another registration folder" type="regular"
        unremovable="true">
        <channel fname="rss" dlm:deleteAllowed="false"
          dlm:editAllowed="false" dlm:moveAllowed="false" unremovable="true"
          hidden="false" immutable="true" ID="n1221" />
      </folder>
    </folder>

    <folder ID="s13" dlm:deleteAllowed="false" dlm:editAllowed="false"
      dlm:moveAllowed="false" hidden="false" immutable="false" name="Footer folder"
      type="footer" unremovable="false" />
  </folder>
</layout>


