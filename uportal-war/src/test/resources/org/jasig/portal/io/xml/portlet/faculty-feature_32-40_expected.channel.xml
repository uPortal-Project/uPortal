<portlet-definition
    xmlns="https://source.jasig.org/schemas/uportal/io/portlet-definition"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://source.jasig.org/schemas/uportal/io/portlet-definition https://source.jasig.org/schemas/uportal/io/portlet-definition/portlet-definition-4.0.xsd"
    version="4.0">
    <ns0:title
        xmlns:ns0="https://source.jasig.org/schemas/uportal/io/portlet-definition">Faculty Welcome</ns0:title>
    <ns1:name
        xmlns:ns1="https://source.jasig.org/schemas/uportal/io/portlet-definition">Faculty Welcome</ns1:name>
    <ns2:fname
        xmlns:ns2="https://source.jasig.org/schemas/uportal/io/portlet-definition">faculty-feature</ns2:fname>
    <ns3:desc
        xmlns:ns3="https://source.jasig.org/schemas/uportal/io/portlet-definition">Faculty-targeted welcome screen</ns3:desc>
    <ns4:type
        xmlns:ns4="https://source.jasig.org/schemas/uportal/io/portlet-definition">Image</ns4:type>
    <ns5:timeout
        xmlns:ns5="https://source.jasig.org/schemas/uportal/io/portlet-definition">30000</ns5:timeout>
    <portlet-descriptor xmlns:up="https://source.jasig.org/schemas/uportal">
        <up:isFramework>true</up:isFramework>
        <up:portletName>Image</up:portletName>
    </portlet-descriptor>
    <ns6:category
        xmlns:ns6="https://source.jasig.org/schemas/uportal/io/portlet-definition">Instructors</ns6:category>

    <ns7:group
        xmlns:ns7="https://source.jasig.org/schemas/uportal/io/portlet-definition">Everyone</ns7:group>



    <ns8:parameter
        xmlns:ns8="https://source.jasig.org/schemas/uportal/io/portlet-definition">
        <ns9:name
            xmlns:ns9="https://source.jasig.org/schemas/uportal/io/portlet-definition">disableDynamicTitle</ns9:name>
        <ns10:value
            xmlns:ns10="https://source.jasig.org/schemas/uportal/io/portlet-definition">true</ns10:value>
        <ns11:description
            xmlns:ns11="https://source.jasig.org/schemas/uportal/io/portlet-definition" />

    </ns8:parameter>
    <parameter>
        <name>editable</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>hasHelp</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>hasAbout</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>secure</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>locale</name>
        <value>false</value>
    </parameter>

    <portlet-preference>
        <ns12:name
            xmlns:ns12="https://source.jasig.org/schemas/uportal/io/portlet-definition">img-border</ns12:name>

        <ns13:value
            xmlns:ns13="https://source.jasig.org/schemas/uportal/io/portlet-definition">0</ns13:value>

    </portlet-preference>
    <portlet-preference>
        <ns14:name
            xmlns:ns14="https://source.jasig.org/schemas/uportal/io/portlet-definition">img-height</ns14:name>

        <ns15:value
            xmlns:ns15="https://source.jasig.org/schemas/uportal/io/portlet-definition">200</ns15:value>

    </portlet-preference>
    <portlet-preference>
        <ns16:name
            xmlns:ns16="https://source.jasig.org/schemas/uportal/io/portlet-definition">img-link</ns16:name>

        <ns17:value
            xmlns:ns17="https://source.jasig.org/schemas/uportal/io/portlet-definition">Faculty_feature.png</ns17:value>

    </portlet-preference>
    <portlet-preference>
        <ns18:name
            xmlns:ns18="https://source.jasig.org/schemas/uportal/io/portlet-definition">img-uri</ns18:name>

        <ns19:value
            xmlns:ns19="https://source.jasig.org/schemas/uportal/io/portlet-definition">media/org/jasig/portal/channels/CImage/faculty_feature.png</ns19:value>

    </portlet-preference>
    <portlet-preference>
        <ns20:name
            xmlns:ns20="https://source.jasig.org/schemas/uportal/io/portlet-definition">img-width</ns20:name>

        <ns21:value
            xmlns:ns21="https://source.jasig.org/schemas/uportal/io/portlet-definition">468</ns21:value>

    </portlet-preference>
</portlet-definition>